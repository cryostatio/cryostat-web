{
	"license":"LICENSE",
	"licenseFormats": {
		"ts|tsx": {
			"prepend": "/*",
			"append": " */",
			"eachLine": {
				"prepend": " * "
			}
		}
	},
	"ignore": [
    ".git",
    "tags",
    "coverage",
    "stories",
    "__mocks__",
    "**/.*",
    "README.md",
    "TESTING.md",
    "**/*.js",
    "yarn.lock",
    ".yarn/**/*",
    ".github/**/*",
    "src/app/assets/*",
    "src/test/**/*.snap"
  ]
}
